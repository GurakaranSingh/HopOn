@inherits DeleteFileBaseClass
    <div class="row">
        <div class="col-lg-12" id="shareTextBox">
            <table id="customers">
                <tr id="FirstHead">
                    <th>File Name</th>
                    <th></th>
                </tr>
                @if (DeleteModel != null)
                {
                    @foreach (var Item in DeleteModel)
                    {
                        <tr id="">
                            <td style="border: 1px solid #ddd;padding: 8px;">  </td>
                            <td>
                                <button type="button" class="btn btn-primary" @onclick="()=>RemoveItemFromList(' + Guid + ')"> Remove Item</button>
                            </td>
                        </tr>
                    }
                }
                    </table>
        </div>
    </div>