@inherits ProgressBarBaseClass
<style>
    .progress {
        width: 50%;
        height: 8px;
    }

    h3.progress-title {
        font-size: 14px;
        margin-bottom: 4px;
    }

    .thumbimg .cross-icn {
        position: absolute;
        top: -10px;
        right: -33px;
        width: 19px;
        height: 45px;
        z-index: 101;
    }

    .progressBlock {
        display: flex;
        width: 100%;
        float: left;
    }

        .progressBlock a {
            width: 30px;
            height: 30px;
            float: left;
            margin-right: 20px;
        }

            .progressBlock a img {
                width: 19px;
                height: 21px;
            }

    .thumbimg {
        width: 222px;
        height: 169px;
        cursor: pointer;
        position: relative;
    }

        .thumbimg .thumbimg-thmb .thmb-pic {
            object-fit: cover;
            width: 222px;
            height: 169px;
        }

    .thumnail_photo .thumbimg {
        width: 222px;
        height: auto;
        cursor: pointer;
        position: relative;
    }


        .thumnail_photo .thumbimg .progress {
            width: 100%;
            height: 8px;
        }

    .thumbimg-thmb {
        width: 222px;
        height: 169px;
        cursor: pointer;
        position: relative;
    }

    .gallery_container {
        width: 100%;
        float: left;
        height: auto;
        overflow-y: auto;
        overflow-x: hidden;
        max-height: 300px;
    }

    .thumnail_photo {
        display: grid;
        grid-gap: 22px;
        grid-template-columns: repeat(3, 1fr);
        margin-bottom: 20px;
    }

        .thumnail_photo .thumbimg-thmb .cross-icn {
            position: absolute;
            top: -6px;
            right: -4px;
            width: 25px;
            height: 25px;
            z-index: 99;
        }
</style>
<div class="thumnail_photo">
    <div id="ListComponent" style="display:none">
    </div>
</div>

<div class="row" id="FileListcomponent">
    <button class="btn btn-success" id="AllDownloadButton" style="display:none" onclick="StartAllDownload()">
        Start Uploading
    </button>
</div>



