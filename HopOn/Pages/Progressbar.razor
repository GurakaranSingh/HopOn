@inherits ProgressBarBaseClass
    <style>
        .progress {
            width: 50%;
            height: 8px;
        }

        h3.progress-title {
            font-size: 14px;
            margin-bottom: 4px;
        }

        .thumbimg .cross-icn {
            position: absolute;
            top: -10px;
            right: -33px;
            width: 19px;
            height: 45px;
            z-index: 101;
        }

        .progressBlock {
            display: flex;
            width: 100%;
            float: left;
        }

            .progressBlock a {
                width: 30px;
                height: 30px;
                float: left;
                margin-right: 20px;
            }

                .progressBlock a img {
                    width: 19px;
                    height: 21px;
                }

       .thumbimg {
            width: 260px;
            height: 169px;
            cursor: pointer;
            position: relative;
        }

             .thumbimg .thmb-pic {
                object-fit: cover;
                width: 260px;
                height: 169px;
            }
    </style>

    <div id="gallery_container">

    </div>
    <div class="row" id="FileListcomponent">
        <button class="btn btn-success" id="AllDownloadButton" style="display:none" onclick="StartAllDownload()">
            Start Uploading
        </button>
    </div>



